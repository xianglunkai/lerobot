{
    "dataset": {
        "repo_id": "pick_tool_into_box",
        "root": null
    },
    "policy": {
        "type": "reward_classifier",
        "repo_id": "reward_classifier",
        "model_name": "/home/xlk/work/lerobot/pretrain_model/resnet10",
        "model_type": "cnn",
        "num_cameras": 1,
        "num_classes": 2,
        "hidden_dim": 256,
        "dropout_rate": 0.1,
        "learning_rate": 1e-4,
        "device": "cuda",
        "use_amp": true,
        "input_features": {
            "observation.images.left": {
                "type": "VISUAL",
                "shape": [
                    3,
                    128,
                    128
                ]
            },
            "observation.state": {
                "type": "STATE",
                "shape": [
                    14
                ]
            }
        }
    },
    "batch_size": 16,
    "num_workers": 4,
    "steps": 5000,
    "log_freq": 10,
    "eval_freq": 1000,
    "save_freq": 1000,
    "save_checkpoint": true,
    "seed": 2,
    "resume": false,
    "optimizer": {
        "grad_clip_norm": 10.0
    },
    "wandb": {
        "enable": false,
        "project": "reward-classifier",
        "disable_artifact": false
    },
    "job_name": "reward-classifier",
    "rename_map": {"observation.images.left": "observation.images.left", "observation.state": "observation.state"}
}